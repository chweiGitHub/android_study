<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/btn_annotation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toAnnotationDemoPage"
        android:text="annotation"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_file"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toFilePage"
        android:text="file"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_annotation" />

    <Button
        android:id="@+id/btn_mvvm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toMvvmPage"
        android:text="DataBinding-Demo"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_file" />


    <Button
        android:id="@+id/btn_CoordinatorLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toCoordinatorLayoutPage"
        android:text="CoordinatorLayout-Demo"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_mvvm" />

    <Button
        android:id="@+id/btn_CoordinatorLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toCoordinatorLayoutPage2"
        android:text="CoordinatorLayout-Demo2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_CoordinatorLayout" />


    <Button
        android:id="@+id/btn_aspect"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toAspectDemoPage"
        android:text="AspectJ-Demo"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_CoordinatorLayout2" />

    <Button
        android:id="@+id/btn_binder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toServicePage"
        android:text="service"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_aspect" />

    <Button
        android:id="@+id/btn_string"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toStringPage"
        android:text="string"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_binder" />

    <Button
        android:id="@+id/btn_games"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toGamePage"
        android:text="to play game"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_string" />

    <Button
        android:id="@+id/btn_tab_viewpager"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toViewPagerPage"
        android:text="tablayout view pager"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_games" />

    <Button
        android:id="@+id/btn_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toToolBarPage"
        android:text="toolbar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_tab_viewpager" />

    <Button
        android:id="@+id/friends_circle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toFriendsCirclePage"
        android:text="friends-circle"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_toolbar" />

    <Button
        android:id="@+id/friends_simple"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toFriendsSimplePage"
        android:text="friends-simple"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_toolbar" />

</androidx.constraintlayout.widget.ConstraintLayout>
